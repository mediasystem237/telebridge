{
    "name": "mbindi/telebridge",
    "description": "The most complete yet lightweight Telegram connector for Laravel. Long polling, notifications, multi-bots, DTOs, and more!",
    "version": "2.2.0",
    "type": "library",
    "keywords": [
        "laravel",
        "telegram",
        "bot",
        "telegram-bot",
        "connector",
        "webhook",
        "telegram-api",
        "laravel-package",
        "php"
    ],
    "homepage": "https://github.com/mbindi/telebridge",
    "license": "MIT",
    "authors": [
        {
            "name": "Mbindi",
            "email": "contact@mbindi.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mbindi\\Telebridge\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mbindi\\Telebridge\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mbindi\\Telebridge\\Providers\\TeleBridgeServiceProvider"
            ],
            "aliases": {
                "TeleBridge": "Mbindi\\Telebridge\\Facades\\TeleBridge"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
